class IShash implements IntSet {
    private HashSet<Integer> hs;

    private int size;

        // Constructor
    IShash () {
        this.hs = new HashSet<Integer> ();
        this.size = 0;
    }
    IShash (int n) {
        this.hs = new HashSet<Integer> ();
        this.hs.add(new Integer(n));
        this.size = 1;
    }

    //@Override
    public void insert (int n) { // insert n into the set
        Integer temp = new Integer(n);
        if (!this.isMember(temp)){
            this.hs.add(temp);
            this.size += 1;
        }
    }

    //@Override
	public boolean isMember (int c) { // whether c is in the set
        return this.hs.contains(new Integer(c));
    }

    //@Override
    public void makeEmpty() {
        this.hs = new HashSet<Integer>();
        this.size = 0;
    }

    //@Override
	public int cardinality() {
        return size;
    }



}
